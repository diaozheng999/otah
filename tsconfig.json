{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "jsx": "react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"],
      "Root": ["src"],
      "Components": ["src/Components"],
      "Components/*": ["src/Components/*"],
      "Core": ["src/Core"],
      "Core/*": ["src/Core/*"],
      "Images": ["src/Images"],
      "Images/*": ["src/Images/*"],
      "Interop": ["src/Interop"],
      "Interop/*": ["src/Interop/*"],
      "Localisation": ["src/Localisation"],
      "Localisation/*": ["src/Localisation/*"],
      "Models": ["src/Models"],
      "Models/*": ["src/Models/*"],
      "Resources": ["src/Resources"],
      "Resources/*": ["src/Resources/*"],
      "Scenes": ["src/Scenes"],
      "Scenes/*": ["src/Scenes/*"],
      "Styles": ["src/Styles"],
      "Styles/*": ["src/Styles/*"],
      "Themes": ["src/Themes"],
      "Themes/*": ["src/Themes/*"]
    },
    "typeRoots": [
      "@types",
      "./typings"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "outDir": "__tscOutput__"
  }
}
